{
  "author": "Duende Software",
  "classifications": [
    "Web",
    "Duende",
    "IdentityServer"
  ],
  "name": "Duende IdentityServer Web Client",
  "identity": "Duende.IdentityServer.WebClient",
  "groupIdentity": "Duende.IdentityServer.WebClient",
  "shortName": [
    "is-webclient"
  ],
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "TemplateWebApp",
  "preferNameDirectory": true,
  "primaryOutputs": [
    {
      "path": "TemplateWebApp.csproj"
    }
  ],
  "symbols": {
    "RenameCommonNamespace": {
      "type": "derived",
      "datatype": "string",
      "replaces": "TemplateWebApp",
      "displayName": "Fix common host namespace.",
      "valueSource": "name",
      "valueTransform": "safe_namespace"
    },

    "Authority": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "{{ Authority }}",
      "displayName": "Identity provider authority URL",
      "defaultValue": "https://demo.duendesoftware.com"
    },
    "ClientId": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "{{ ClientId }}",
      "displayName": "Identity provider client ID",
      "defaultValue": "interactive.confidential"
    },
    "ClientSecret": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "{{ ClientSecret }}",
      "displayName": "Identity provider client secret",
      "defaultValue": "secret"
    },
    "Scopes": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "{{ Scopes }}",
      "displayName": "Identity provider scopes",
      "defaultValue": "openid profile email"
    }
  }
}
