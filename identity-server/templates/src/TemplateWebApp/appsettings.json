{
  "IdentityProvider": {
    "Authority": "{{ Authority }}",
    "ClientId": "{{ ClientId }}",
    "ClientSecret": "{{ ClientSecret }}",
    "Scopes": "{{ Scopes }}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
